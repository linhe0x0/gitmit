{
  "name": "gitmit",
  "version": "1.1.1",
  "description": "gitmit is a interactive command line tool so that you can easily use emoji and conventional commit format.",
  "keywords": [
    "git",
    "commit",
    "emoji",
    "gitmoji"
  ],
  "author": "sqrtthree <imsqrtthree@gmail.com>",
  "homepage": "https://github.com/sqrthree/gitmit#readme",
  "bugs": {
    "url": "https://github.com/sqrthree/gitmit/issues"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqrthree/gitmit.git"
  },
  "preferGlobal": true,
  "main": "bin/gitmit",
  "bin": {
    "gitmit": "bin/gitmit"
  },
  "files": [
    "bin",
    "src"
  ],
  "scripts": {
    "test": "NODE_ENV='testing' ava",
    "cover": "NODE_ENV='testing' nyc ava",
    "lint": "eslint src --cache"
  },
  "dependencies": {
    "axios": "~0.23.0",
    "chalk": "~4.1.2",
    "co": "~4.6.0",
    "commander": "~2.19.0",
    "execa": "~5.1.1",
    "fuzzy": "~0.1.3",
    "inquirer": "~5.2.0",
    "inquirer-autocomplete-prompt": "~0.12.2",
    "ora": "~5.4.1",
    "update-notifier": "~2.5.0",
    "word-wrap": "1.2.3"
  },
  "devDependencies": {
    "@sqrtthree/prettier-config": "~2.0.1",
    "ava": "~3.15.0",
    "coveralls": "~3.1.1",
    "del": "~6.0.0",
    "eslint": "~7.32.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-import": "~2.25.2",
    "eslint-plugin-prettier": "~4.0.0",
    "mock-fs": "~5.1.1",
    "nyc": "~11.7.3",
    "pre-commit": "~1.2.2",
    "prettier": "~2.4.1",
    "rewire": "~4.0.1",
    "should": "~13.2.3",
    "tempfile": "~2.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ]
}
