{
  "name": "gitmit",
  "version": "0.2.0",
  "description": "gitmit is a interactive command line tool so that you can easily use emoji and conventional commit format.",
  "keywords": [
    "git",
    "commit",
    "emoji",
    "gitmoji"
  ],
  "author": "sqrtthree <imsqrtthree@gmail.com>",
  "homepage": "https://github.com/sqrthree/gitmit#readme",
  "bugs": {
    "url": "https://github.com/sqrthree/gitmit/issues"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sqrthree/gitmit.git"
  },
  "preferGlobal": true,
  "main": "bin/gitmit",
  "bin": {
    "gitmit": "bin/gitmit"
  },
  "scripts": {
    "test": "NODE_ENV='testing' ava",
    "cover": "NODE_ENV='testing' nyc ava",
    "lint": "eslint src --cache"
  },
  "dependencies": {
    "axios": "^0.17.0",
    "chalk": "^2.0.1",
    "co": "^4.6.0",
    "commander": "^2.9.0",
    "execa": "^0.8.0",
    "inquirer": "^5.0.0",
    "ora": "^1.1.0",
    "update-notifier": "^2.1.0",
    "word-wrap": "1.2.3"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "mock-fs": "^4.2.0",
    "nyc": "^11.0.3",
    "pre-commit": "^1.2.2",
    "rewire": "^3.0.0",
    "should": "^13.1.1",
    "temp-write": "^3.1.0",
    "tempfile": "^2.0.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ]
}
